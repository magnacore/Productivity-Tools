rsync --delete --progress -vahi @(from_path + "Desktop/") ~/Desktop/
rsync --delete --progress -vahi @(from_path + "Documents/") ~/Documents/
rsync --delete --progress -vahi @(from_path + "Downloads/") ~/Downloads/
rsync --delete --progress -vahi @(from_path + "Installers/") ~/Installers/
rsync --delete --progress -vahi @(from_path + "Music/") ~/Music/
