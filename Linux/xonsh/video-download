#!/home/manuj/anaconda3/envs/xonsh/bin/xonsh

# source-bash ~/anaconda3/etc/profile.d/conda.sh

# conda activate util

$HOME/anaconda3/envs/util/bin/youtube-dl -c --merge-output-format 'mkv' -f 'bestvideo[height<=1080]+bestaudio' -o '%(playlist)s/%(playlist_index)s - %(title)s.%(ext)s' --write-auto-sub -a ~/Backups/youtube.txt
